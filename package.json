{
    "private": true,
    "workspaces": {
        "packages": [
            "addons/shared/*/*",
            "packages/pyradic/platform"
        ]
    },
    "scripts": {
        "reinstall": "rimraf \"**/node_modules\" \"**/yarn.lock\" && yarn",
        "build:dev": "NODE_ENV=development webpack --config webpack.config.ts",
        "build:prod": "NODE_ENV=production webpack --production --config webpack.config.ts",
        "serve": "NODE_ENV=development webpack-dev-server --inline --hot --config webpack.config.ts",
        "platform": "cd packages/pyradic/platform && yarn",
        "admin-theme": "cd addons/shared/pyro/admin-theme && yarn",
        "ex1": "cd addons/shared/examples/ex1 && yarn",
        "ex2": "cd addons/shared/examples/ex2 && yarn",
        "packages:watch": "concurrently \"yarn build-tools watch\""
    },
    "dependencies": {
        "@babel/core": "^7.6.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.6.0",
        "@babel/preset-env": "^7.6.0",
        "@radic/webpacker": "6.4.1",
        "@types/babel-core": "^6.25.6",
        "@types/debug": "^4.1.5",
        "@types/dotenv": "^6.1.1",
        "@types/eventemitter2": "^4.1.0",
        "@types/friendly-errors-webpack-plugin": "^0.1.2",
        "@types/glob": "^7.1.1",
        "@types/lodash": "^4.14.141",
        "@types/lz-string": "^1.3.33",
        "@types/mini-css-extract-plugin": "^0.8.0",
        "@types/node": "^12.7.5",
        "@types/node-sass": "^4.11.0",
        "@types/pug": "^2.0.4",
        "@types/sass": "^1.16.0",
        "@types/stylus": "^0.48.31",
        "@types/tapable": "^1.0.4",
        "@types/webpack": "^4.39.1",
        "@types/webpack-bundle-analyzer": "^2.13.2",
        "@types/webpack-dev-server": "^3.1.7",
        "@types/webpackbar": "^2.6.0",
        "@vue/babel-preset-app": "^3.11.0",
        "@vue/composition-api": "^0.3.2",
        "babel-loader": "^8.0.6",
        "babel-plugin-component": "^1.1.1",
        "babel-plugin-import": "^1.12.1",
        "concurrently": "^4.1.2",
        "css-loader": "^3.2.0",
        "debug": "^4.1.1",
        "dotenv": "^8.1.0",
        "element-ui": "^2.12.0",
        "expose-loader": "^0.7.5",
        "file-loader": "^4.2.0",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "glob": "^7.1.4",
        "lodash": "^4.17.15",
        "lz-string": "^1.4.4",
        "mini-css-extract-plugin": "^0.8.0",
        "node-sass": "^4.12.0",
        "npm-run-all": "^4.1.5",
        "package-json-type": "^1.0.3",
        "pug": "^2.0.4",
